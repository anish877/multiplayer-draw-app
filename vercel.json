{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "http-backend/dist/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "ws-backend/dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "http-backend/dist/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH, OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type, Authorization"
        }
      },
      {
        "src": "/ws",
        "dest": "ws-backend/dist/index.js",
        "methods": ["GET"],
        "headers": {
          "Connection": "Upgrade",
          "Upgrade": "websocket"
        }
      },
      {
        "src": "/(.*)",
        "dest": "frontend/.next/$1"
      }
    ],
    "functions": {
      "http-backend/dist/index.js": {
        "runtime": "nodejs18.x"
      },
      "ws-backend/dist/index.js": {
        "runtime": "nodejs18.x"
      }
    }
  }
  